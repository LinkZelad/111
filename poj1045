#include <iostream>
#include <cstdio>
#include <cmath>
#include <iomanip>
using namespace std;

int main()
{
    int n, i;
    double vs, r, c, w, vr;
    cin >> vs >> r >> c >> n;
    for (i = 0; i < n; i++)
    {
        cin >> w;
        vr = c * r * w * vs / sqrt(1+(c*r*w*c*r*w));
        cout.setf(ios::fixed);
        cout << setprecision(3) << vr <<endl;
    }
    return 0;
}
